package practicalwork;

public class Task6 {
    public static void main(String[] args) {
        System.out.println("Первые 10 чисел гармонического ряда:");
        
        for (int n = 1; n <= 10; n++) {
            double harmonicNumber = 0.0;
            for (int i = 1; i <= n; i++) {
                harmonicNumber += 1.0 / i;
            }
            System.out.printf("H(%d) = %.4f%n", n, harmonicNumber);
        }
    }
}
